create_table("users", func(t) {
	t.Column("id", "uuid", {"primary": true})
	t.Column("user", "string", {})
	t.Column("email", "string", {})
	t.Column("password_hash", "string", {})
	t.Column("first_name", "string", {"null": true})
	t.Column("last_name", "string", {"null": true})
	t.Column("admin", "bool", {})
})
